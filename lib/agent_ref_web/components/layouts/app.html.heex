<div class="h-screen flex">
  <div class="w-64 shrink-0 flex flex-col h-full bg-gray-900">
    <div class="flex grow flex-col gap-y-5 overflow-y-auto px-6">
      <div class="flex items-center h-24 mt-6 mb-4">
        <img
          class="h-16 w-auto mr-4"
          src={~p"/images/White Logo.png"}
          alt="Premier Listings Company Logo"
        />
        <div class="text-white font-bold leading-tight text-center">
          <div>Agent</div>
          <div>Referral</div>
          <div>Network</div>
        </div>
      </div>
      <nav class="flex flex-1 flex-col">
        <ul role="list" class="flex flex-1 flex-col gap-y-7">
          <li>
            <ul role="list" class="-mx-2 space-y-1">
              <li>
                <.link
                  navigate={~p"/"}
                  class={"group flex gap-x-3 rounded-md p-2 text-sm font-semibold leading-6 #{if @live_action == :index, do: "text-white", else: "text-gray-400 hover:bg-gray-800 hover:text-white"}"}
                  style={"#{if @socket.view == AgentRefWeb.DashLive.Index, do: "background-color: #0db6bf;", else: ""}"}
                >
                  <svg
                    class="h-6 w-6 shrink-0"
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke-width="1.5"
                    stroke="currentColor"
                    aria-hidden="true"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M2.25 12l8.954-8.955c.44-.439 1.152-.439 1.591 0L21.75 12M4.5 9.75v10.125c0 .621.504 1.125 1.125 1.125H9.75v-4.875c0-.621.504-1.125 1.125-1.125h2.25c.621 0 1.125.504 1.125 1.125V21h4.125c.621 0 1.125-.504 1.125-1.125V9.75M8.25 21h8.25"
                    />
                  </svg>
                  Home
                </.link>
              </li>
              <li>
                <.link
                  navigate={~p"/agents"}
                  class={"group flex gap-x-3 rounded-md p-2 text-sm font-semibold leading-6 #{if @live_action == :index, do: "text-white", else: "text-gray-400 hover:bg-gray-800 hover:text-white"}"}
                  style={"#{if @socket.view == AgentRefWeb.AgentLive.Index, do: "background-color: #0db6bf;", else: ""}"}
                >
                  <svg
                    class="h-6 w-6 shrink-0"
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke-width="1.5"
                    stroke="currentColor"
                    aria-hidden="true"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M15 19.128a9.38 9.38 0 002.625.372 9.337 9.337 0 004.121-.952 4.125 4.125 0 00-7.533-2.493M15 19.128v-.003c0-1.113-.285-2.16-.786-3.07M15 19.128v.106A12.318 12.318 0 018.624 21c-2.331 0-4.512-.645-6.374-1.766l-.001-.109a6.375 6.375 0 0111.964-3.07M12 6.375a3.375 3.375 0 11-6.75 0 3.375 3.375 0 016.75 0zm8.25 2.25a2.625 2.625 0 11-5.25 0 2.625 2.625 0 015.25 0z"
                    />
                  </svg>
                  Referral Agents
                </.link>
              </li>
              <li>
                <.link
                  navigate={~p"/map"}
                  class={"group flex gap-x-3 rounded-md p-2 text-sm font-semibold leading-6 #{if @live_action == :index, do: "text-white", else: "text-gray-400 hover:bg-gray-800 hover:text-white"}"}
                  style={"#{if @socket.view == AgentRefWeb.MapLive.Index, do: "background-color: #0db6bf;", else: ""}"}
                >
                  <svg
                    class="h-6 w-6 shrink-0"
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke-width="1.5"
                    stroke="currentColor"
                    aria-hidden="true"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M2.25 12.75V12A2.25 2.25 0 014.5 9.75h15A2.25 2.25 0 0121.75 12v.75m-8.69-6.44l-2.12-2.12a1.5 1.5 0 00-1.061-.44H4.5A2.25 2.25 0 002.25 6v12a2.25 2.25 0 002.25 2.25h15A2.25 2.25 0 0021.75 18V9a2.25 2.25 0 00-2.25-2.25h-5.379a1.5 1.5 0 01-1.06-.44z"
                    />
                  </svg>
                  Map
                </.link>
              </li>
              <li>
                <.link
                  navigate={~p"/"}
                  class={"group flex gap-x-3 rounded-md p-2 text-sm font-semibold leading-6 #{if @live_action == :index, do: "text-white", else: "text-gray-400 hover:bg-gray-800 hover:text-white"}"}
                  style={"#{if @socket.view == AgentRefWeb.DashLive.Index, do: "background-color: #0db6bf;", else: ""}"}
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke-width="1.5"
                    stroke="currentColor"
                    class="size-6"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M3.75 12h16.5m-16.5 3.75h16.5M3.75 19.5h16.5M5.625 4.5h12.75a1.875 1.875 0 0 1 0 3.75H5.625a1.875 1.875 0 0 1 0-3.75Z"
                    />
                  </svg>
                  FAQ
                </.link>
              </li>
            </ul>
          </li>
          <li class="-mx-6 mt-auto">
            <a
              href="#"
              class="flex items-center gap-x-4 px-6 py-3 text-sm font-semibold leading-6 text-white hover:bg-gray-800"
            >
              <button
                type="button"
                class="rounded-md bg-white px-3 py-2 text-sm font-semibold text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 hover:bg-gray-50"
              >
                + Add a new agent
              </button>
              <img
                class="h-8 w-8 rounded-full bg-gray-800"
                src="https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80"
                alt=""
              />
              <span class="sr-only">You're signed in as</span>
              <span aria-hidden="true">Dezirae Fernandez</span>
            </a>
          </li>
        </ul>
      </nav>
    </div>
  </div>
  <div class="flex-grow bg-[#e1e1e1]">
    <%= @inner_content %>
  </div>
</div>
